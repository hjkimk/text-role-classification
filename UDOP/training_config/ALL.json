{
    "no_cuda": false,
    "debug_mode": false,
    "num_gpu": 1,
    "loading_script": "/UDOP/dataset.py",
    "datasets": ["ICPR2022Real", "CHIME-R", "DeGruyter", "EconBiz"],
    "model_name": "udop_unimodel_large_224",
    "pretrained_model": "",
    "classifier": "linear",
    "classifier_dropout": null,
    "hidden_dropout_prob": 0.1,
    "special_tokens": ["\u201c", "\u201d"],
    "metric": "seqeval",
    "return_entity_level_metrics": false,
    "combine_type": "concat",
    "eval_mode": false,
    "transforms_augmentations": null,
    "transforms_balancing": null,
    "concat_datasets": false,
    "experiment_mode": false,
    "output_dir": "/UDOP/outputs",
    "max_steps": 20000,
    "warmup_steps": 1000,
    "batch_size": 2,
    "learning_rate": [2e-4],
    "weight_decay": 1e-2,
    "adam_beta1": 0.9,
    "adam_beta2": 0.98,
    "logging_dir": "/UDOP/logs",
    "logging_strategy": "steps",
    "logging_steps": 1000,
    "gradient_accumulation_steps": 8
}